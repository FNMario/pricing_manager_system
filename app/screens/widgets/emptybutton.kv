#:kivy 2.2.1

<EmptyButton>:
    background_normal: ''
    background_down: 'atlas://data/images/defaulttheme/button_pressed'
    background_color: (.13, .14, .19, 1)
    color: (.2, .64, .81, 1)
    canvas.before:
        Color:
            rgba: (.2, .64, .81, 1)
        Line:
            rounded_rectangle: (self.pos[0],self.pos[1],self.size[0],self.size[1], 4)
            width: 2